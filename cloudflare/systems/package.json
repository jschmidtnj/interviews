{
  "name": "cloudflare-systems",
  "version": "1.0.0",
  "description": "cloudflare interview systems assignment",
  "main": "dist/worker.js",
  "scripts": {
    "build": "webpack",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "test": "jest --config jestconfig.json --verbose",
    "preview": "concurrently \"yarn build --watch\" \"wrangler preview --watch --env development --url https://postit.monster\"",
    "dev": "concurrently \"yarn build --watch\" \"wrangler dev --env development --port 3002\"",
    "deploy": "wrangler publish",
    "deploy:dev": "wrangler publish --env development",
    "logs": "wrangler tail"
  },
  "keywords": [
    "systems",
    "cloudflare"
  ],
  "author": "Joshua Schmidt",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.0.0",
    "@types/itty-router-extras": "^0.4.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "concurrently": "^6.3.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prettier": "^2.3.0",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^27.0.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "dependencies": {
    "http-status-codes": "^2.1.4",
    "itty-router": "^2.4.4",
    "itty-router-extras": "^0.4.2"
  }
}
